</span><span class='line'>            <span class="p">{</span>
</span><span class='line'>                <span class="c1">//匹配http://www.phpchina.com/bbs/viewthread.php?tid=68127&amp;amp; extra=page%3D1&amp;amp;page=1&amp;amp;sid=iBLZfK</span>
</span><span class='line'>                <span class="k">if</span> <span class="p">(</span><span class="o">!</span><span class="nb">preg_match</span><span class="p">(</span><span class="s2">&quot;/^(http:\/\/www.phpchina.com\/bbs\/viewthread.php\?tid=)[0-9]&lt;/em&gt;&amp;amp;extra=page\%3D1&amp;amp;page=[0-9]&lt;em&gt;&amp;amp;sid=[a-zA-Z]{6}/i&quot;</span><span class="p">,</span><span class="nv">$value</span><span class="p">))</span>
